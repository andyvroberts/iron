<NavigationWindow x:Class="Obdurate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Obdurate"
        mc:Ignorable="d"
        Title="Obdurate Home" Height="350" Width="525"
        ShowsNavigationUI="False" ResizeMode="CanResizeWithGrip"
        Source="views\Start.xaml">
  <NavigationWindow.Style>
    <Style TargetType="{x:Type NavigationWindow}">
      <Setter Property="Background" Value="{StaticResource WindowBrush}"/>
    </Style>
  </NavigationWindow.Style>
</NavigationWindow>